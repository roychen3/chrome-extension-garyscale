{
  "manifest_version": 3,
  "name": "Grayscale",
  "version": "1.0",
  "description": "",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "utils/index.js",
        "scripts/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
